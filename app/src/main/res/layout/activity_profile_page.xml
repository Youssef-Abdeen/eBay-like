<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rating"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".LenoraUS9ProfilePage.ProfilePage"
    tools:visibility="visible">
    <!--I see you added an image, but it is not displaying on the emulator.
     Also, cannot see the users name displayed - Dave-->
    <Button
        android:id="@+id/goToLogOutPageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="294dp"
        android:layout_marginBottom="15dp"
        android:text="LogOut"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/goToLandingPageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="15dp"
        android:text="HOME"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="128dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="33dp"
        android:cropToPadding="true"
        android:scaleX="5"
        android:scaleY="5"
        android:src="@drawable/baseline_account_circle_24"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/displayUserName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="81dp"
        android:text="username"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/fullName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:layout_marginEnd="102dp"
        android:text="name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayUserName"
        />


    <Button
        android:id="@+id/notifGood"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="16dp"
        android:text="Goods Near Me"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/goToLogOutPageButton"
        app:layout_constraintHorizontal_bias="0.359"
        app:layout_constraintStart_toEndOf="@+id/goToLandingPageButton" />

    <TextView
        android:id="@+id/tradeouttext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="207dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="3dp"
        android:text="Trade out Value:"
        android:textColor="@color/black"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/tradeoutvalue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/tradeintext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Trade in Value:"
        android:textStyle="bold"
        android:textSize="22sp"
        android:textColor="@color/black"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/tradeoutvalue"
        app:layout_constraintStart_toStartOf="@+id/tradeouttext"/>

    <TextView
        android:id="@+id/tradeoutvalue"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:textSize="21sp"
        android:textColor="@color/black"
        android:layout_marginStart="15dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/tradeouttext"
        app:layout_constraintStart_toStartOf="@+id/tradeouttext"/>

    <TextView
        android:id="@+id/tradeinvalue"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:textSize="21sp"
        android:textColor="@color/black"
        android:layout_marginStart="15dp"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/tradeintext"
        app:layout_constraintStart_toStartOf="@+id/tradeouttext"/>

    <TextView
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:text="$"
        android:textSize="21sp"
        android:textColor="@color/black"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/tradeintext"
        app:layout_constraintStart_toStartOf="@+id/tradeouttext"/>

    <TextView
        android:layout_width="15dp"
        android:layout_height="wrap_content"
        android:text="$"
        android:textSize="21sp"
        android:textColor="@color/black"
        android:layout_marginTop="3dp"
        app:layout_constraintTop_toBottomOf="@+id/tradeouttext"
        app:layout_constraintStart_toStartOf="@+id/tradeouttext"/>

    <Button
        android:id="@+id/getRating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="200dp"
        android:layout_marginBottom="85dp"
        android:text="Get My Rating"
        app:layout_constraintBottom_toTopOf="@+id/tradeouttext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />


</androidx.constraintlayout.widget.ConstraintLayout>